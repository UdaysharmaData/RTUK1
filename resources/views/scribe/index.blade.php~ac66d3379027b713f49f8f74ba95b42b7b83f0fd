    --form "categories[][local_fee]=41" \
    --form "categories[][international_fee]=95" \
    --form "categories[][start_date]=11-11-2022" \
    --form "categories[][end_date]=11-11-2022" \
    --form "categories[][registration_deadline]=28-10-2022" \
    --form "categories[][withdrawal_deadline]=21-10-2022" \
    --form "type=standalone" \
    --form "charities=all" \
    --form "status=" \
    --form "fundraising_emails=1" \
    --form "classic_membership_places=1" \
    --form "premium_membership_places=11" \
    --form "exclude_charities=" \
    --form "exclude_website=" \
    --form "exclude_participants=1" \
    --form "withdrawals=" \
    --form "archived=" \
    --form "route_" \
    --form "what_is_" \
    --form "how_to_get_" \
    --form "event_day_" \
    --form "spectator_" \
    --form "kit_" \
    --form "estimated=1" \
    --form "total_places=8" \
    --form "charity_checkout_integration=1" \
    --form "socials[][platform]=twitter" \
    --form "socials[][url]=https://twitter.com/bathhalf" \
    --form "meta[title]=Bath Half Marathon" \
    --form "meta[description]=The Bath Half Marathon has been re-scheduled from 29th May 2022 to 16th October 2022.
                The Bath Half Marathon is one of the longest established and most popular city-centre road events in the UK and it's back for 2022." \
    --form "meta[keywords][]=bath half" \
    --form "route_info_media[]=@/private/var/folders/jx/nlpz41b53zv_3dyh6c2k29pr0000gn/T/phpPxMy2n" \
    --form "what_is_included_media[]=@/private/var/folders/jx/nlpz41b53zv_3dyh6c2k29pr0000gn/T/phpsCXggv" \
    --form "image=@/Users/daveabiola/code/api/public/images/emails/sport-for-charity.jpg" \
    --form "gallery[]=@/private/var/folders/jx/nlpz41b53zv_3dyh6c2k29pr0000gn/T/phpSkRXtU" </code></pre></div>


