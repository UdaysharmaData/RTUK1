    "Content-Type": "multipart/form-data",
    "Accept": "application/json",
};

const body = new FormData();
body.append('name', 'Bath Half Marathon 749');
body.append('description', 'sit');
body.append('partner_event', '');
body.append('location', 'Fourth Floor, Maya House, 134-138 Borough High St');
body.append('city', 'Westminster');
body.append('region', 'london');
body.append('postcode', 'SE1 1LB');
body.append('country', 'England');
body.append('reminder', 'daily');
body.append('categories[][slug]', 'marathons');
body.append('categories[][local_fee]', '41');
body.append('categories[][international_fee]', '95');
body.append('categories[][start_date]', '11-11-2022');
body.append('categories[][end_date]', '11-11-2022');
body.append('categories[][registration_deadline]', '28-10-2022');
body.append('categories[][withdrawal_deadline]', '21-10-2022');
body.append('type', 'standalone');
body.append('charities', 'all');
body.append('status', '');
body.append('fundraising_emails', '1');
body.append('classic_membership_places', '1');
body.append('premium_membership_places', '11');
body.append('exclude_charities', '');
body.append('exclude_website', '');
body.append('exclude_participants', '1');
body.append('withdrawals', '');
body.append('archived', '');
body.append('route_info', '');
body.append('what_is_included', '');
body.append('how_to_get_there', '');
body.append('event_day_logistics', '');
body.append('spectator_info', '');
body.append('kit_list', '');
body.append('estimated', '1');
body.append('total_places', '8');
