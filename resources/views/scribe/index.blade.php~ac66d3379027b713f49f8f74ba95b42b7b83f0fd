body.append('meta[description]', 'The Bath Half Marathon has been re-scheduled from 29th May 2022 to 16th October 2022.
                The Bath Half Marathon is one of the longest established and most popular city-centre road events in the UK and it's back for 2022.');
body.append('meta[keywords][]', 'bath half');
body.append('route_info_media[]', document.querySelector('input[name="route_info_media[]"]').files[0]);
body.append('what_is_included_media[]', document.querySelector('input[name="what_is_included_media[]"]').files[0]);
body.append('image', document.querySelector('input[name="image"]').files[0]);
body.append('gallery[]', document.querySelector('input[name="gallery[]"]').files[0]);

fetch(url, {
    method: "POST",
    headers,
    body,
}).then(response =&gt; response.json());</code></pre></div>

