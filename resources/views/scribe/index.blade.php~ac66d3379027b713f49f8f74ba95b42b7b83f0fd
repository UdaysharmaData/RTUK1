            <h2 id="events-PUTapi-v1-portal-events--id--update">Update an event</h2>

<p>
<small class="badge badge-darkred">requires authentication</small>
</p>



<span id="example-requests-PUTapi-v1-portal-events--id--update">
<blockquote>Example request:</blockquote>


<div class="bash-example">
    <pre><code class="language-bash">curl --request PUT \
    "https://api.test/api/v1/portal/events/334/update" \
    --header "Authorization: Bearer {YOUR_AUTH_KEY}" \
    --header "Content-Type: multipart/form-data" \
    --header "Accept: application/json" \
    --form "name=Bath Half Marathon 237" \
    --form "description=consectetur" \
    --form "partner_event=" \
    --form "location=Fourth Floor, Maya House, 134-138 Borough High St" \
    --form "city=Westminster" \
    --form "region=london" \
    --form "postcode=SE1 1LB" \
    --form "country=England" \
    --form "reminder=daily" \
    --form "categories[][slug]=marathons" \
    --form "categories[][local_fee]=15" \
    --form "categories[][international_fee]=59" \
    --form "categories[][start_date]=11-11-2022" \
    --form "categories[][end_date]=11-11-2022" \
    --form "categories[][registration_deadline]=28-10-2022" \
    --form "categories[][withdrawal_deadline]=21-10-2022" \
    --form "type=standalone" \
    --form "charities=all" \
    --form "status=" \
    --form "fundraising_emails=" \
    --form "classic_membership_places=12" \
    --form "premium_membership_places=2" \
    --form "exclude_charities=1" \
    --form "exclude_website=1" \
    --form "exclude_participants=1" \
    --form "withdrawals=1" \
    --form "archived=" \
    --form "route_" \
    --form "what_is_" \
    --form "how_to_get_" \
    --form "event_day_" \
    --form "spectator_" \
    --form "kit_" \
    --form "estimated=1" \
    --form "total_places=18" \
    --form "charity_checkout_integration=" \
    --form "socials[][platform]=twitter" \
    --form "socials[][url]=https://twitter.com/bathhalf" \
    --form "meta[title]=Bath Half Marathon" \
    --form "meta[description]=The Bath Half Marathon has been re-scheduled from 29th May 2022 to 16th October 2022.
                The Bath Half Marathon is one of the longest established and most popular city-centre road events in the UK and it's back for 2022." \
    --form "meta[keywords][]=bath half" \
    --form "route_info_media[]=@/private/var/folders/jx/nlpz41b53zv_3dyh6c2k29pr0000gn/T/phpyj5eIC" \
    --form "what_is_included_media[]=@/private/var/folders/jx/nlpz41b53zv_3dyh6c2k29pr0000gn/T/phpjg4xaO" \
    --form "image=@/Users/daveabiola/code/api/public/images/emails/sport-for-charity.jpg" \
    --form "gallery[]=@/private/var/folders/jx/nlpz41b53zv_3dyh6c2k29pr0000gn/T/phpvgWtRY" </code></pre></div>


<div class="javascript-example">
    <pre><code class="language-javascript">const url = new URL(
    "https://api.test/api/v1/portal/events/334/update"
);

const headers = {
    "Authorization": "Bearer {YOUR_AUTH_KEY}",
    "Content-Type": "multipart/form-data",
    "Accept": "application/json",
};

const body = new FormData();
body.append('name', 'Bath Half Marathon 237');
body.append('description', 'consectetur');
body.append('partner_event', '');
body.append('location', 'Fourth Floor, Maya House, 134-138 Borough High St');
body.append('city', 'Westminster');
body.append('region', 'london');
body.append('postcode', 'SE1 1LB');
body.append('country', 'England');
body.append('reminder', 'daily');
body.append('categories[][slug]', 'marathons');
body.append('categories[][local_fee]', '15');
body.append('categories[][international_fee]', '59');
body.append('categories[][start_date]', '11-11-2022');
body.append('categories[][end_date]', '11-11-2022');
body.append('categories[][registration_deadline]', '28-10-2022');
body.append('categories[][withdrawal_deadline]', '21-10-2022');
body.append('type', 'standalone');
body.append('charities', 'all');
body.append('status', '');
body.append('fundraising_emails', '');
body.append('classic_membership_places', '12');
body.append('premium_membership_places', '2');
body.append('exclude_charities', '1');
body.append('exclude_website', '1');
body.append('exclude_participants', '1');
body.append('withdrawals', '1');
body.append('archived', '');
body.append('route_info', '');
body.append('what_is_included', '');
body.append('how_to_get_there', '');
body.append('event_day_logistics', '');
body.append('spectator_info', '');
body.append('kit_list', '');
body.append('estimated', '1');
body.append('total_places', '18');
body.append('charity_checkout_integration', '');
body.append('socials[][platform]', 'twitter');
body.append('socials[][url]', 'https://twitter.com/bathhalf');
body.append('meta[title]', 'Bath Half Marathon');
body.append('meta[description]', 'The Bath Half Marathon has been re-scheduled from 29th May 2022 to 16th October 2022.
                The Bath Half Marathon is one of the longest established and most popular city-centre road events in the UK and it's back for 2022.');
body.append('meta[keywords][]', 'bath half');
body.append('route_info_media[]', document.querySelector('input[name="route_info_media[]"]').files[0]);
body.append('what_is_included_media[]', document.querySelector('input[name="what_is_included_media[]"]').files[0]);
body.append('image', document.querySelector('input[name="image"]').files[0]);
body.append('gallery[]', document.querySelector('input[name="gallery[]"]').files[0]);

fetch(url, {
    method: "PUT",
    headers,
    body,
}).then(response =&gt; response.json());</code></pre></div>

