    "Accept": "application/json",
};

let body = {
    "enquiry_type": "race_entries_north",
    "full_name": "Wendy Bird",
    "email": "user@email.com",
    "message": "I need to make an enquiry..."
};

fetch(url, {
    method: "POST",
    headers,
    body: JSON.stringify(body),
}).then(response =&gt; response.json());</code></pre></div>

