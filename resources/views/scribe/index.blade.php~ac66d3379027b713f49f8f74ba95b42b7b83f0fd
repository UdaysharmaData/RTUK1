    "https://api.test/api/v1/media-collections/store" \
    --header "Content-Type: application/json" \
    --header "Accept: application/json" \
    --data "{
    \"name\": \"tkxmhqgyzknuhifowfjdg\",
    \"description\": \"\"
}"
</code></pre></div>


